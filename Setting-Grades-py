score = input('Enter Score: ')

try:
    sc = float(score)
except:
    sc = -1
    
if sc > 1.0:
    print('Please use a number between 1.0 and 0.0.')
elif sc >= 0.9:
    print('A')
elif sc >= 0.8:
    print('B')
elif sc >= 0.7:
    print('C')
elif sc >= 0.6:
    print('D')
elif sc >= 0.0:
    print('F')
else:
    print('Please use a number between 1.0 and 0.0.')
